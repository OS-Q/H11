{
  "name": "H5",
  "title": "H5",
  "description": "TIVA开发平台",
  "homepage": "https://github.com/OS-Q/H5",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "<5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OS-Q/H5.git"
  },
  "version": "1.0.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-N8",
      "script": "builder/frameworks/energia.py"
    },
    "energia": {
      "package": "framework-N8",
      "script": "builder/frameworks/energia.py"
    },
    "libopencm3": {
      "package": "framework-N4",
      "script": "builder/frameworks/libopencm3/libopencm3.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.40803.0,<1.40805.0"
    },
    "framework-N8": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N8.git"
    },
    "framework-N4": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/OS-Q/N4.git"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    }
  }
}
